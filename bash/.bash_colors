
# defines colours in constants
export COL_BLACK="\033[0;30m"
export COL_DGRAY="\033[1;30m"
export COL_RED="\033[0;31m"
export COL_LRED="\033[1;31m"
export COL_GREEN="\033[0;32m"
export COL_LGREEN="\033[1;32m"
export COL_BROWN="\033[0;33m"
export COL_YELLOW="\033[1;33m"
export COL_BLUE="\033[0;34m"
export COL_LBLUE="\033[1;34m"
export COL_PURPLE="\033[0;35m"
export COL_PINK="\033[1;35m"
export COL_DCYAN="\033[0;36m"
export COL_CYAN="\033[1;36m"
export COL_LGRAY="\033[0;36m"
export COL_WHITE="\033[1;36m"
export COL_RESET="\033[0m"

print_colors() {
	for attr in 0 1 4 5 7 ; do
		echo "----------------------------------------------------------------"
		printf "ESC[%s;Vordergrund;Hintergrundm - \n" $attr
		for fore in 30 31 32 33 34 35 36 37; do
			for back in 40 41 42 43 44 45 46 47; do
				printf '\033[%s;%s;%sm %02s;%02s  ' $attr $fore $back $fore $back
			done
		printf '\n'
		done
		printf '\033[0m'
	done
}
