# This is Git's per-user configuration file.

#
[user]
	name = <name>
	email = <email>
	signingKey = <gpg-key>
#
[http]
	proxy = <proxy>
#
[alias]
	f = fetch
	c = commit
	s = status
	p = pull
	b = branch
	co = checkout
	cp = cherry-pick
	amend = commit --amend
	amend2 = amend --no-edit
	cdiff = diff --cached
#
[protocol]
	version = 2
#
[core]
	autocrlf = input
	pager = less -x1,5
#
[commit]
	gpgSign = true
#
[merge]
	conflictStyle = diff3
#
[log]
	showSignature = true
#
[rebase]
	autoStash = true
	missingCommitsCheck = error
#
[push]
	default = nothing
#
[gc]
	auto = 500
#
[pack]
	threads = 2
#
[gui]
	tabsize = 4
#
